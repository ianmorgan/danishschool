<h1>Change Password</h1>


 <% form_for @member, :url => { :action => 'update_password', :id => @member.id } do |f| %>

  <%= f.error_messages %>
  
  
  <p>
      <b><%= f.label :password %></b><br />
      <%= f.password_field :password %>   
  </p>
 
  
  <%= f.password_field :password_confirmation %>   


  <p>
    <%= f.submit 'Change Password' %>
  </p>
<% end %>

